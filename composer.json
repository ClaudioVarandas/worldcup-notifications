{
    "name": "kadinho/worldcup-notifications",
    "description": "A project to fetch FIFA World Cup matches and send event notifications.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "worldcup-notifications"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kadinho/worldcup-notifications/issues",
        "source": "https://github.com/kadinho/worldcup-notifications"
    },
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/database": "5.6.*",
        "laravel-zero/framework": "5.6.*",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["wc2018-notifications"]
}
